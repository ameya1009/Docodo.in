// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  name      String
  business  String
  email     String
  budget    String
  goal      String
  message   String?
  createdAt DateTime @default(now())
}

model Newsletter {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
}

model GrowthGraderLead {
  id        String   @id @default(cuid())
  email     String
  answers   Json
  createdAt DateTime @default(now())
}
